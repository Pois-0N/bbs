#tt_um_login a img{ margin-left:5px;}
#tt_um_login .login{ float:right;}

/*已登录样式*/
.tt_userinfo{ float:right; margin-top:10px; position:relative;}
.tt_userinfo .pipe{ display:none}
.tt_userinfo li {float: left;position: relative;}
.tt_userinfo .tt_author > a{display:inline-block;border:3px solid #F1F2F6;height:34px;padding:10px 10px 0 10px; text-align:center; line-height:34px; color:#999;width:110px;}
.tt_userinfo .tt_author span{ margin-right:5px; position:relative; top:-6px;}
.tt_userinfo .tt_author img{ width:24px; height:24px; border-radius:12px;}
.tt_userinfo .tt_author:hover > a{background:#F2F2F2;border:3px solid {YANSE1}; border-bottom:none;}
.tt_userinfo .son{position:absolute; background:#fff;left:1px;top:43px; display:none; z-index:10;*margin-right:4px; border:3px solid {YANSE1};border-top:none; }
.tt_userinfo .son a{ display:inline-block;line-height:32px; text-align:center; background:#fff;border-bottom:1px solid #FAFAFA; cursor:default;color:#666; width:100%;background: no-repeat;background-position: 8px 8px;}
.tt_userinfo .son a:hover{ background:#FAFAFA;text-decoration:none;background: no-repeat;background-position: 8px 8px; cursor:pointer; color:#ff0000;}
.tt_userinfo .tt_author:hover .son{width:130px; display:block;right:0;left:auto;}
.tt_userinfo .son a img{width: auto;height: auto;border-radius: 0;padding: 5px 0;}

/*快捷导航*/
#qmenu {width: 83px;height: 26px;line-height: 26px;margin: 4px 10px 0 0;padding: 0 12px 0 0;}
#qmenu.a {background-position: 0 -26px;}
#qmenu_menu {width: 240px;margin-top: 0;}
#qmenu{ margin-top:10px}
#qmenu.qmenu_tt {width: 31px;height: 26px;line-height: 26px;margin: 9px 10px 0 0;padding: 0;border-top: 1px solid transparent;border-left: 1px solid transparent;border-radius: 5px;background: url({STYLEIMGDIR}/qmenu.png) center no-repeat;text-indent: -9999px;}
#qmenu.qmenu_tt.a {border-color:{YANSE1};background:{YANSE2} url({STYLEIMGDIR}/qmenu.png) center no-repeat;}
#qmenu_menu.qmenu_menu_tt {position: relative;width: 180px;margin-top: 14px;padding: 20px 10px 10px;border: 1px solid {YANSE2};border-radius: 5px;background: #fff;box-shadow: none;}
#qmenu_menu.qmenu_menu_tt:after {content: "";position: absolute;right: 10px;top: -4px;height: 0;width: 0;border-color: transparent transparent #fff transparent;border-style: dashed dashed solid dashed;border-width: 0 4px 4px;}
#qmenu_menu.qmenu_menu_tt:before {content: "";position: absolute;right: 9px;top: -5px;height: 0;width: 0;border-color: transparent transparent {YANSE2} transparent;border-style: dashed dashed solid dashed;border-width: 0 5px 5px;}
#qmenu_menu.qmenu_menu_tt ul.nav a {width: 45px;margin-bottom: 8px;background-position: center 0;}

