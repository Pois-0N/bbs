select{background:none;background-color: #FFF;border: 1px solid #CCC;box-shadow: 0px 1px 1px rgba(0, 0, 0, 0.075) inset;transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;padding:0px;font-size: 14px;color: #555;border-radius: 3px}
.pt{ background:none;background-color: #FFF;border: 1px solid #CCC;box-shadow: 0px 1px 1px rgba(0, 0, 0, 0.075) inset;transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;padding: 4px 6px;font-size: 14px;line-height: 18px;color: #555;border-radius: 3px}
#block_setting {}

/* 页头 */
#toptb { min-width: 960px; border-bottom:0px solid {COMMONBORDER}; background:#E6E9ED; line-height: 28px; color:#656d78; }
#toptb a { float: left; padding: 0 4px; height: 28px; }
#toptb a.showmenu { padding-right: 15px; }
#toptb a.hover { background-color: #FFF; }
#toptb .pipe { float: left; display: none; }

/*导航样式*/
#nv { overflow: hidden;   height: 45px; background:{YANSE1}; }
#nv li { float: left; padding-right: 1px; height: 45px; line-height: 45px; background:none; font-weight:400; font-size: 15px;}
.ie_all #nv li { line-height: 48px; }
.ie6 #nv li { line-height: 45px; }
#nv li a { float: left; padding: 0 15px; height: 45px;}
#nv li a { color:#fff; }
#nv li span { display: none; }
#nv li.a { margin-left: -1px; background:{YANSE2}; }
#nv li.a a { color:#fff; }
#nv li a:hover { background:{YANSE2}; }
#nv li.hover a:hover, #nv li.hover a { background:{YANSE2}; }

/*搜索样式*/
.tt_search {float: right;margin: 9px 5px 0 0;}
.tt_search #scbar {height: 26px;border:0; border-radius:4px;background:{YANSE2};}
.tt_search .scbar_type_outer {display: none;}
.tt_search #scbar_type {height: 100%;padding-left: 0;background: url({STYLEIMGDIR}/arrwd.gif) no-repeat right center;}
.tt_search #scbar_txt {float: left;width: 105px;height: 26px;line-height: 26px;border: none;background: transparent;color: #fafafa !important;font-size: 12px;text-indent: 5px;-webkit-transition: 0.4s width;-moz-transition: 0.4s width;transition: 0.4s width;}
.tt_search #scbar_txt::-webkit-input-placeholder {color: #FFA88B !important;}
.tt_search #scbar_btn {float: left;width: 30px;height: 26px;margin: 0;background: transparent url({STYLEIMGDIR}/searchicon.png) center center no-repeat;border: none;text-indent: -9999px;cursor: pointer;}
.tt_search #scbar.fold {width: 30px;padding: 0;border-color: transparent;cursor: pointer;background: transparent url({STYLEIMGDIR}/searchicon.png) center center no-repeat;}
*html .tt_search #scbar.fold{ border:none}
.tt_search #scbar.fold #scbar_txt {width: 0;}
.tt_search #scbar.fold #scbar_btn {display: none;}
.tt_search #scbar.fold:hover {background:url({STYLEIMGDIR}/searchicon.png) no-repeat center center;}


/*未登录样式*/
